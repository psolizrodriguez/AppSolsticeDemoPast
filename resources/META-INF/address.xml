<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<query name="com.solstice.contact.dao.impl.AddressDaoImpl.listAll">
		<![CDATA[FROM Address]]>
	</query>
	<query name="com.solstice.contact.dao.impl.AddressDaoImpl.getById">
		<![CDATA[FROM Address a WHERE a.addressId = ?1]]>
	</query>
</hibernate-mapping>
